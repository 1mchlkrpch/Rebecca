/* This file is
Similar to YACC-file you can define variables by value in
double quotes and it will be value that will compare with
types in token sequence.*/

// Token names type.
name = "TOKEN_NAME";

// multiplication devision
primary_opr_rule
	: '*'
	| '/'
	| secondary_opr
	;

// 
secondary_opr
	: '+'
	| '-'
	| parentheses
	;

parentheses
	: primary_opr_rule
	| name
	;
